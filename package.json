{
  "name": "mutastate",
  "version": "2.0.16",
  "description": "Mutable state management",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "jest",
    "dev": "NODE_ENV=development rollup -c rollup.config.js && npm run copyextras",
    "build": "NODE_ENV=production rollup -c rollup.config.js && npm run copyextras",
    "copyextras": "cp ./src/index.d.ts ./dist/index.d.ts",
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose"
  },
  "keywords": [
    "nexuscript",
    "object",
    "mutastate",
    "nexusoft",
    "wizebin"
  ],
  "author": "wizebin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wizebin/mutastate.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-numeric-separator": "^7.10.1",
    "@babel/plugin-proposal-throw-expressions": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.4",
    "jsdoc-export-default-interop": "^0.3.1",
    "minami": "^1.2.3",
    "mock-local-storage": "^1.1.12",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "clone": "^2.1.1",
    "lodash.clone": "^4.5.0",
    "objer": "^1.0.46"
  }
}
